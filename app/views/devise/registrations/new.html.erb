<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>
<div class="container">
	<div class ="jumbotron">
		<h2 class="text-center">Sign Up</h2>
		<form role="form">				
			
		  <div class="form-group"><%= f.label :username %><br />
		  <%= f.text_field :username, :autofocus => true, :class =>'input' %></div>

		  <div class="form-group"><%= f.label :email %><br />
		  <%= f.email_field :email, :autofocus => true, :class =>'input' %></div>

		  <div class="form-group"><%= f.label :password %><br />
		  <%= f.password_field :password, :class =>'input' %></div>

		  <div class="form-group"><%= f.label :password_confirmation %><br />
		  <%= f.password_field :password_confirmation, :class =>'input' %></div>

		<div><%= f.submit "Sign up", :class => 'btn btn-small btn-primary' %></div>
		<% end %>

			<div class = "links"><%= render "devise/shared/links" %></div>
		</form>
	</div>
</div>
